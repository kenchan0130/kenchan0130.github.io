    "lint:fix": "run-p lint:fix:*",
    "lint:dryrun": "run-p lint:dryrun:*",
    "start": "webpack --mode development --devtool source-map --watch | bundle exec jekyll serve --future",
    "build": "NODE_ENV=production webpack --mode production"
